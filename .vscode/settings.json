{
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "eslint.validate": ["javascript", "javascriptreact"],
  "files.encoding": "utf8",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    ".cache/**": true,
    "node_modules/**": true,
    "build/**": true,
    "public/build/**": true
  }
}
